{
    "compilerOptions": {
    "module": "none",
    "target": "es5",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,    
    "noImplicitAny": false,
    "sourceMap": true,
    "watch": false,
    "outDir": "wwwroot",
    "outFile": "wwwroot/app.js",
    "jsx": "react"
  },
  "exclude": [
    "node_modules",
    "_DocumentDB"
  ],
  "compileOnSave": true
}